[bootstrap]
main_collection = /example/example.collectionc
render = /builtins/render/default.renderc

[script]
shared_state = 1

[display]
width = 960
height = 640

[project]
title = ImGui
custom_resources = example/bundle/

[library]
include_dirs = imgui

[input]
game_binding = /imgui/bindings/imgui.input_bindingc
use_accelerometer = 0

[ios]
bundle_identifier = com.defold.imgui

[android]
package = com.defold.imgui

[native_extension]
app_manifest = /opengl.appmanifest

